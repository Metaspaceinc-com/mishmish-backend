{
    "name": "mishmish-api",
    "version": "1.0.0",
    "description": "Property rental platform API with shift-based bookings",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon src/server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:integration": "jest tests/integration",
        "test:performance": "jest tests/performance",
        "test:ci": "jest --coverage --ci --watchAll=false",
        "migrate": "node src/database/migrate.js",
        "seed": "node src/database/seed.js",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "docker:build": "docker build -t mishmish-api .",
        "docker:run": "docker run -p 3000:3000 mishmish-api"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "compression": "^1.7.4",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.3.1",
        "@supabase/supabase-js": "^2.39.0",
        "redis": "^4.6.10",
        "bull": "^4.12.2",
        "axios": "^1.6.2",
        "multer": "^1.4.5-lts.1",
        "sharp": "^0.32.6",
        "uuid": "^9.0.1",
        "moment": "^2.29.4",
        "winston": "^3.11.0",
        "express-winston": "^4.2.0",
        "joi": "^17.11.0",
        "node-cron": "^3.0.3",
        "twilio": "^4.19.3",
        "nodemailer": "^6.9.7",
        "stripe": "^14.9.0"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.55.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "rental",
        "property",
        "booking",
        "api",
        "nodejs",
        "express",
        "supabase"
    ],
    "author": "Mishmish Team",
    "license": "MIT"
}